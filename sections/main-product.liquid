<script type="lazyload_int">
  // Filters product cards on page load based on color variant alt tags
  document.querySelectorAll('[data-thumbnail-color]').forEach(function (img) {
    img.style.display = 'none';
  });

  var selectedColor = document.querySelector('input[name="Colour"]:checked');
  var selected_color = selectedColor.className;
  var thumbnail_selector = document.querySelectorAll('[data-thumbnail-color="' + selected_color + '"]');
  thumbnail_selector.forEach(function (img) {
    img.style.display = 'block';
  });
</script>

<script type="lazyload_int">
  {% comment %} This listens for a change in product variant, and if it exists, resets all the Media Gallery Slider Components {% endcomment %}
  var firstTimeUrl = document.URL;
  document.addEventListener('change', function() {
    var currentPageUrl = document.URL;
    var url = new URL(currentPageUrl);
    var isVariantUrl = url.searchParams.get("variant");
    currentPageUrl = isVariantUrl ? currentPageUrl : isVariantUrl;
      if(currentPageUrl && firstTimeUrl != currentPageUrl) {
        firstTimeUrl = currentPageUrl;

        let mediaGalleryWithVertical = document.querySelector('media-gallery-with-vertical-slider');
        if (mediaGalleryWithVertical != null) {
          mediaGalleryWithVertical.querySelectorAll('[id^="GalleryVerticalThumbnails-"], [id^="GalleryThumbnails-"]').forEach(slider => slider.resetPages());
        };

        let mediaGalleryHorzintalOnly = document.querySelector('media-gallery');
        if (mediaGalleryHorzintalOnly != null) {
          mediaGalleryHorzintalOnly.querySelector('[id^="GalleryThumbnails-"]').resetPages();
        }
      }
  });
</script>

