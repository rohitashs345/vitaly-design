<link rel="stylesheet" data-href="{{ 'section-main-product.css' | asset_url }}">
<link rel="stylesheet" data-href="{{ 'component-accordion.css' | asset_url }}">
<link rel="stylesheet" data-href="{{ 'component-price.css' | asset_url }}">
<link rel="stylesheet" data-href="{{ 'component-rte.css' | asset_url }}">
<link rel="stylesheet" data-href="{{ 'component-slider.css' | asset_url }}">
<link rel="stylesheet" data-href="{{ 'component-rating.css' | asset_url }}">
<link rel="stylesheet" data-href="{{ 'component-loading-overlay.css' | asset_url }}">
