    modelViewerTransform.orientation = `${rolly}deg ${pitchy}deg ${yawy}deg`;

    const updateOrientation = () => {
      modelViewerTransform.orientation = `${roll.value}deg ${pitch.value}deg ${yaw.value}deg`;
    };

    roll.addEventListener('input', () => {
      updateOrientation();
    });
    pitch.addEventListener('input', () => {
      updateOrientation();
    });
    yaw.addEventListener('input', () => {
      updateOrientation();
    });
  </script>
{% endcomment %}

<script type="lazyload_int">
  const modelViewerExists = document.querySelectorAll('model-viewer').length > 0;
  if (modelViewerExists) {
    function onModelClick() {
      var miniModelWrapper = document.querySelector('.product__model-wrapper');
      var miniModel = document.querySelector('.product__model-wrapper model-viewer');
      var sliderButtons = document.querySelector('.slider-buttons');
      var modelViewerClose = document.querySelector('.model-viewer-close');
      var productPageWidth = document.querySelector('.product-page').scrollWidth;
      var modelViewerOpen = document.querySelector('.model-viewer-open-desktop');

      miniModelWrapper.classList.add('modelViewer-opened');
      miniModelWrapper.classList.remove('modelViewer-closed');

      sliderButtons.style.display = 'none';

      miniModelWrapper.setAttribute('visible', 'opened');

      modelViewerOpen.style.display = 'none';
      if (productPageWidth >= 990) {
        miniModelWrapper.style.border = 'none';
        modelViewerClose.style.display = 'block';
      }
    }

    function onModelExitClick() {
      var miniModelWrapper = document.querySelector('.product__model-wrapper');
      var miniModel = document.querySelector('.product__model-wrapper model-viewer');
      var sliderButtons = document.querySelector('.slider-buttons');
      var modelViewerClose = document.querySelector('.model-viewer-close');
      var modelViewerOpenMobile = document.querySelector('.model-viewer-open-mobile');
      var modelViewerOpen = document.querySelector('.model-viewer-open-desktop');

      miniModelWrapper.classList.add('modelViewer-closed');
      miniModelWrapper.classList.remove('modelViewer-opened');

      miniModelWrapper.setAttribute('visible', 'closed');

      sliderButtons.style.display = 'flex';

      modelViewerClose.style.display = 'none';
      modelViewerOpen.style.display = 'flex';
    }

    function onModelOpenClickMobile() {
      var miniModelWrapper = document.querySelector('.product__model-wrapper');
      var modelViewerOpenMobile = document.querySelector('.model-viewer-open-mobile');
      var modelViewerCloseMobile = document.querySelector('.model-viewer-close-mobile');
      var sliderButtons = document.querySelector('.slider-buttons');
      miniModelWrapper.style.display = 'block';
      modelViewerOpenMobile.style.display = 'none';
      modelViewerCloseMobile.style.display = 'block';
      sliderButtons.style.display = 'none';

      miniModelWrapper.classList.add('modelViewer-opened');
      miniModelWrapper.classList.remove('modelViewer-closed');

      const modelViewer = document.querySelector('model-viewer');
      modelViewer.addEventListener('load', function () {
        const model = document.querySelector('model-viewer');

        const colorString = '1, 1, 1, 1';

        const color = colorString.split(',').map((numberString) => parseFloat(numberString));

        const metalnessDisplay = 1;

        const roughnessDisplay = 0;

        model.model.materials.forEach(changeMaterial);

        function changeMaterial(material) {
          material.pbrMetallicRoughness.setBaseColorFactor(color);
          material.pbrMetallicRoughness.setMetallicFactor(metalnessDisplay);
          material.pbrMetallicRoughness.setRoughnessFactor(roughnessDisplay);
        }
      });
    }

    function onModelExitClickMobile() {
      var miniModelWrapper = document.querySelector('.product__model-wrapper');
      var modelViewerOpenMobile = document.querySelector('.model-viewer-open-mobile');
      var modelViewerCloseMobile = document.querySelector('.model-viewer-close-mobile');
      var sliderButtons = document.querySelector('.slider-buttons');

      miniModelWrapper.classList.add('modelViewer-closed');
      miniModelWrapper.classList.remove('modelViewer-opened');

      miniModelWrapper.style.display = 'none';
      modelViewerOpenMobile.style.display = 'flex';
      modelViewerCloseMobile.style.display = 'none';
      sliderButtons.style.display = 'flex';
    }

    //       model viewer change material (colour and metallness)
    const modelViewer = document.querySelector('model-viewer');
    modelViewer.addEventListener('load', function () {
      const model = document.querySelector('model-viewer');

      const colorString = '1, 1, 1, 1';

      const color = colorString.split(',').map((numberString) => parseFloat(numberString));

      const metalnessDisplay = 1;

      const roughnessDisplay = 0;

      model.model.materials.forEach(changeMaterial);

      function changeMaterial(material) {
        material.pbrMetallicRoughness.setBaseColorFactor(color);
        material.pbrMetallicRoughness.setMetallicFactor(metalnessDisplay);
        material.pbrMetallicRoughness.setRoughnessFactor(roughnessDisplay);
      }
    });
  }
</script>

<script type="lazyload_int">
